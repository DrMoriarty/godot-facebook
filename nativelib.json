{
  "name": "facebook",
  "display_name": "Facebook SDK",
  "description": "Facebook plugin",
  "version": "9.3.0",
  "license": "MIT",
  "url": "https://github.com/DrMoriarty/godot-facebook",
  "author": {
    "name": "DrMoriarty",
    "url": "https://github.com/DrMoriarty",
    "donate": "https://ko-fi.com/drmoriarty"
  },
  "godot_version": "3.3.0",
  "category": "misc",
  "dependencies": [
    "nativelib-export"
  ],
  "variables": {
    "Facebook/FB_APP_ID": {
      "info": "Set your Facebook App ID here"
    }
  },
  "files": {
    "scripts": "scripts",
    "nativelib-export": "addons/nativelib-export"
  },
  "autoload": {
    "facebook": "*res://scripts/facebook.gd"
  },
  "platform_ios": {
    "files": {
      "ios-framework/facebook.gdip": "ios/plugins/facebook/",
      "ios-framework/build/facebook.debug.xcframework": "ios/plugins/facebook/facebook.debug.xcframework",
      "ios-framework/build/facebook.release.xcframework": "ios/plugins/facebook/facebook.release.xcframework",
      "ios-framework/plugin/FBSDKCoreKit.framework": "ios/plugins/facebook/FBSDKCoreKit.framework",
      "ios-framework/plugin/FBSDKLoginKit.framework": "ios/plugins/facebook/FBSDKLoginKit.framework",
      "ios-framework/plugin/FBSDKShareKit.framework": "ios/plugins/facebook/FBSDKShareKit.framework"
    }
  },
  "platform_android": {
    "files": {
      "android": "android/facebook"
    },
    "android_module": "org/godotengine/godot/GodotFacebook"
  }
}
